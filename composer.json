{
    "name": "SelimSalihovic/currencyshifts",
    "description": "The free, complete exchange rates JSON API.",
    "keywords": ["exchange rates", "currencies", "conversion"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/psr-http-message-bridge": "0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CurrencyShifts\\": "currencyshifts/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
          "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
